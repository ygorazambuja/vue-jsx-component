{
  "name": "vue-jsx-component-creator",
  "displayName": "Vue Component Creator",
  "description": "Create Vue Components",
  "version": "0.0.5",
  "publisher": "ygorazambuja",
  "repository": {
    "url": "https://github.com/ygorazambuja/vue-jsx-component"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vue-jsx-component.helloWorld",
    "onCommand:vue-jsx-component.generateFiles"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "group": "VueJSX",
          "command": "vue-jsx-component.generateFiles",
          "title": "VueJSX - Create Component"
        }
      ]
    },
    "commands": [
      {
        "command": "vue-jsx-component.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vue-jsx-component.generateFiles",
        "title": "VueJSX - Create Component"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "@vscode/test-electron": "^1.6.2"
  }
}
